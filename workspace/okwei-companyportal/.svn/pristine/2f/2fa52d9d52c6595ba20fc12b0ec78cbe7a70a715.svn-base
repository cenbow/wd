/* Tab切换 */
.container {width: 99.8%;float: left;}
ul.tab {float: left;list-style: none;height: 32px;border-bottom: 1px solid #ddd;border-left: 1px solid #ddd;width: 100%;}
ul.tab li {float: left;height: 31px;line-height: 31px;border: 1px solid #ddd;border-left: none;margin-bottom: -13px;background: #e0e0e0;overflow: hidden;position: relative;}
ul.tab li.active, ul.tab li.active a:hover {background: #fff;border-bottom: 1px solid #fff;color: #ff0000;}
ul.tab li.active a{background: #fff;border-bottom: 1px solid #fff;color: #ff0000;}
ul.tab li a {text-decoration: none;color: #000;display: block;font-size: 1.2em;padding: 0 20px;/* border: 1px solid blue; */outline: none;}
.tab_container {
	border: 1px solid #ddd;
	border-top: none;
	clear: both;
	float: left;
	width: 100%;
	background: #fff;
	-moz-border-radius-bottomright: 5px;
	-khtml-border-radius-bottomright: 5px;
	-webkit-border-bottom-right-radius: 5px;
	-moz-border-radius-bottomleft: 5px;
	-khtml-border-radius-bottomleft: 5px;
	-webkit-border-bottom-left-radius: 5px;
}
.tab_content {font-size: 1.2em;}
/* Tab切换End */


