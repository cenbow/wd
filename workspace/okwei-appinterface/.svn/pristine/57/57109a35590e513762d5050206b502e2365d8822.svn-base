package com.okwei.appinterface.bean.vo.order;

import java.util.Date;

public class BookAssistModel
{
    //全额、百分比、预付金额
	private Short bookPayType;
	//百分比
	private Short percent;
	//定金
	private Double amount;
	//0发货前、1发货后
	private Short tailPayType;
	//发送时间
	private Date preSendTime;
	//是否付订金  0表示未付订金1付订金
	private short paydeposit;
	//是否已付尾款   0未付尾款 1已付尾款
	private short paydeadline;
	//是否修改价格   0未修改价格  1修改价格
	private short ModifyPrice;
	//订单尾款
	private Double balancepayment;
	//尾款支付订单号
	private String tOrder;
	//首款支付订单号
	private String hOrder;
	public Short getBookPayType()
	{
		return bookPayType;
	}
	public void setBookPayType(Short bookPayType) {
		this.bookPayType = bookPayType;
	}
	public Double getAmount() {
		return amount;
	}
	public void setAmount(Double amount) {
		this.amount = amount;
	}
	public Short getTailPayType() {
		return tailPayType;
	}
	public void setTailPayType(Short tailPayType) {
		this.tailPayType = tailPayType;
	}
	public Date getPreSendTime() {
		return preSendTime;
	}
	public void setPreSendTime(Date preSendTime) 
	{
		this.preSendTime = preSendTime;
	}
	public short getPaydeposit() {
		return paydeposit;
	}
	public void setPaydeposit(short paydeposit) {
		this.paydeposit = paydeposit;
	}
	public short getPaydeadline() 
	{
		return paydeadline;
	}
	public void setPaydeadline(short paydeadline) {
		this.paydeadline = paydeadline;
	}
	public short getModifyPrice() {
		return ModifyPrice;
	}
	public void setModifyPrice(short modifyPrice) {
		ModifyPrice = modifyPrice;
	}
	public Short getPercent() {
		return percent;
	}
	public void setPercent(Short percent) {
		this.percent = percent;
	}
	public Double getBalancepayment() {
		return balancepayment;
	}
	public void setBalancepayment(Double balancepayment)
	{
		this.balancepayment = balancepayment;
	}
	public String gettOrder() {
		return tOrder;
	}
	public void settOrder(String tOrder) {
		this.tOrder = tOrder;
	}
	public String gethOrder() {
		return hOrder;
	}
	public void sethOrder(String hOrder) {
		this.hOrder = hOrder;
	}
}
