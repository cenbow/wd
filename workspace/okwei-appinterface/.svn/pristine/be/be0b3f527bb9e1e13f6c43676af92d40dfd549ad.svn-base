package com.okwei.appinterface.bean.vo;

import java.io.Serializable;
import java.util.List;

import com.okwei.bean.vo.SaleAreaModel;

public class APPHomeVO implements Serializable {

    private SaleAreaModel flashSaleArea;
    private SaleAreaModel specialSaleArea;
    private List<SaleAreaModel> shoppingAreaList;

    public SaleAreaModel getFlashSaleArea() {
	return flashSaleArea;
    }

    public void setFlashSaleArea(SaleAreaModel flashSaleArea) {
	this.flashSaleArea = flashSaleArea;
    }

    public SaleAreaModel getSpecialSaleArea() {
	return specialSaleArea;
    }

    public void setSpecialSaleArea(SaleAreaModel specialSaleArea) {
	this.specialSaleArea = specialSaleArea;
    }

    public List<SaleAreaModel> getShoppingAreaList() {
	return shoppingAreaList;
    }

    public void setShoppingAreaList(List<SaleAreaModel> shoppingAreaList) {
	this.shoppingAreaList = shoppingAreaList;
    }
}
