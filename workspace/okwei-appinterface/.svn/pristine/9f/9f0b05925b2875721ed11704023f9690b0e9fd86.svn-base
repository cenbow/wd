package com.okwei.appinterface.bean.vo.order;

public class PayParamVO {

	//支付订单号
	public String orderNo;
	//支付密码（des加密）
	public String pwd;
	//手机验证码
	public String vCode;
	//支付签名
	public String sign;
	//图片验证码
	public String checkCode;
	
	//是否钱包全额支付（0全额，1 部分支付）
	public int isFullPay;
	//来源( 1:app 2: wap 3: pc)
	public int payFrom;
	
	public int isCoin;
	//代金券金额
	public double coinAmount;
	
	//是否用微金币支付
	public int useOkweiCoin;
	
	//排序
	public String payOrder;
	
	
	
	
	
	
	
	public int getUseOkweiCoin() {
		return useOkweiCoin;
	}
	public void setUseOkweiCoin(int useOkweiCoin) {
		this.useOkweiCoin = useOkweiCoin;
	}
	public String getPayOrder() {
		return payOrder;
	}
	public void setPayOrder(String payOrder) {
		this.payOrder = payOrder;
	}
	public double getCoinAmount() {
		return coinAmount;
	}
	public void setCoinAmount(double coinAmount) {
		this.coinAmount = coinAmount;
	}
	public int getIsCoin() {
		return isCoin;
	}
	public void setIsCoin(int isCoin) {
		this.isCoin = isCoin;
	}
	public String getOrderNo() {
		return orderNo;
	}
	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
	public String getvCode() {
		return vCode;
	}
	public void setvCode(String vCode) {
		this.vCode = vCode;
	}
	public String getSign() {
		return sign;
	}
	public void setSign(String sign) {
		this.sign = sign;
	}
	public String getCheckCode() {
		return checkCode;
	}
	public void setCheckCode(String checkCode) {
		this.checkCode = checkCode;
	}
	public int getPayFrom() {
		return payFrom;
	}
	public void setPayFrom(int payFrom) {
		this.payFrom = payFrom;
	}
	public int getIsFullPay() {
		return isFullPay;
	}
	public void setIsFullPay(int isFullPay) {
		this.isFullPay = isFullPay;
	}
	
	
}
