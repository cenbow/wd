package com.okwei.appinterface.bean.vo;

import java.io.Serializable;

public class ProductDomain implements Serializable{
	
	private Long productId ;    //商品Id
	private String goodsName ;  //商品名称
	private String goodsPic ;   //商品图片
	private Integer parId ;     //商品所属招商系列的编号
	private String groundPrice; //落地价，如果是落地店显示数字价格，否则赋给“落地店可见”;
	private Float agentPrice ;  //代理价
	private Float salePrice ;   //零售价
	private int stock ;         //库存量
	private Float groundLine;   //成为该商品所属系列落地的最低采购价
	private int saleTotal ;     //销量
	private short relation ;    //当前
	
	public ProductDomain(){
		
	}

	public Long getProductId() {
		return productId;
	}

	public void setProductId(Long productId) {
		this.productId = productId;
	}

	public String getGoodsName() {
		return goodsName;
	}

	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}

	public String getGoodsPic() {
		return goodsPic;
	}

	public void setGoodsPic(String goodsPic) {
		this.goodsPic = goodsPic;
	}

	public Integer getParId() {
		return parId;
	}

	public void setParId(Integer parId) {
		this.parId = parId;
	}

	public String getGroundPrice() {
		return groundPrice;
	}

	public void setGroundPrice(String groundPrice) {
		this.groundPrice = groundPrice;
	}

	public Float getAgentPrice() {
		return agentPrice;
	}

	public void setAgentPrice(Float agentPrice) {
		this.agentPrice = agentPrice;
	}

	public Float getSalePrice() {
		return salePrice;
	}

	public void setSalePrice(Float salePrice) {
		this.salePrice = salePrice;
	}

	public int getStock() {
		return stock;
	}

	public void setStock(int stock) {
		this.stock = stock;
	}

	public Float getGroundLine() {
		return groundLine;
	}

	public void setGroundLine(Float groundLine) {
		this.groundLine = groundLine;
	}

	public int getSaleTotal() {
		return saleTotal;
	}

	public void setSaleTotal(int saleTotal) {
		this.saleTotal = saleTotal;
	}

	public short getRelation() {
		return relation;
	}

	public void setRelation(short relation) {
		this.relation = relation;
	}

}
