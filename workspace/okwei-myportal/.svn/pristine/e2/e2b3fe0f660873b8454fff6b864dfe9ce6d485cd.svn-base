/*----------------window 按钮图标提示信息--------------------- */
.msg-right,.msg-wrong,.msg-warn,.msg-affirm,.msg-user{background: url(../images/imggroup/msg.gif) no-repeat; width:48px; height:48px; margin:10px 20px; display:block;}
.msg-right { background-position:0 0; }
.msg-wrong {background-position:0 -50px; }
.msg-warn {background-position:0 -100px;}
.msg-affirm {background-position:0 -150px;}
.msg-user { background-position:0 -200px;}
.msg-bg { border:1px solid #ffc198; background-color:#feffd6;}
