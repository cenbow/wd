@charset "utf-8";
/* 公共样式 */
body,html,ul,li,dl,dt,dd,ol,h1,h2,h3,h4,h5,h6,p,form,input,th,td{margin:0px;padding:0px;}
/* 主体样式 */
body{font:12px/18px Arial,"宋体";color:#333;background:#fff;}

/* 图片与表单*/
img{border:0px;vertical-align:top;}
/* 去点*/
ul,li{list-style-type:none;}
/* 链接样式*/
a{color:#666;text-decoration:none;}
a:hover{color:#f00;text-decoration:none;}
/* 公共调用*/
.clear{clear:both;height:0px;font:0px/0px;overflow:hidden;line-height:0px;}
.left{float:left;}
.right{float:right;}
.fb{font-weight:bold;}
.orange{color:#F47A02;font-size:14px;font-family:Arial;}
.red{color:#f00;}
em{font-style:normal;}
a.h-color{color:#666 !important;margin:0 0 0 10px;}
a.h-color:hover{color:#f00 !important;}	
.g-color{color:#54a400 !important;}
.g-color:hover{color:#f00 !important;}


/*=============== 头部 =====================*/
.top{width:100%;margin:0 auto;padding:20px 0 0 0;*padding:20px 0 18px 0;padding-top: 25px;height: 75px;background: #fff;border-bottom: 1px solid #D9D9D9;}
	.topLogo{float:left;margin:0 0 18px 0;}
