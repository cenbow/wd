/*分类导航*/
.tabContainer { float:left;width:100%; height:44px; }
.tab{width:100%; height:50px; display: -moz-box;position:relative;display: -webkit-box;display: box;-moz-box-orient: horizontal;-webkit-box-orient: horizontal;box-orient: horizontal;overflow:hidden;}

.tab:after{content: "";position: absolute;width: 100%;height: 62px;bottom:0;left: 0;background: url(../images/mzh_bolang_bg.gif) repeat-x;background-size: 105px 8px;}
.swiper-slide{width:auto;float: left;}/*取消掉插件样式的100%，重要*/
.tab a{height:25px;color:#804055;font-size:14px;display:inline-block;margin:7px 0 0 10px; padding:3px 20px 3px 20px;border-radius:15px;line-height:25px;text-align:center;text-decoration: none;}
.tab .swiper-slide:last-child a{margin-right:25px;}/*修复最后一个无法完全显示*/
.tab a:hover{color:#fff;background-color:#de0102;text-shadow: #ffffff 0 0px 0;box-shadow:0px 3px 8px #d64e78 inset;}
.tab .current{float:left;color:#fff;background-color:#de0102;text-shadow: #ffffff 0 0px 0;box-shadow:0px 3px 8px #d64e78 inset;}
/*.fixed-swiper-wrapper{position:fixed;left:0;top:0;z-index:222;background:#f5edef; opacity:0.9; width:100%;}*//*导航滚动定位效果*/
.fixed-swiper-wrapper{position:fixed;left:0;top:0;z-index:222;background:rgba(239,239,239,0.8);width:100%;}/*导航滚动定位效果*/
.tab.fixed-swiper-wrapper:after{opacity:0.8;}

.mzh_lb {float: left;width: 100%;height: 165px;overflow: hidden;}
.mzh_lb li{float: left;width: 100%;}
.mzh_lb li.mzh_dw_lb{width: 100%;position:absolute;left:0px;bottom:0px;background: #767676;height: 0.5em;}
.mzh_lb li.mzh_dw_lb span.mzh_dw_lb_yes{width: 50%;float: left;border-bottom: 0.5em solid #ff33ab;}

.lileft{float: left;margin-left: 3%;width: 45.5%;}
.lileft .lidiv{float: left; background-color: #ffffff;border: 1px solid #d1d1d1;border-radius: 7px;box-shadow: 1px 1px 3px #d5d5d5;box-sizing: border-box;margin: 10px 0 0;position: relative;width: 100%;}
.lileft .lidiv .liimg{text-align: center;}
.lileft .lidiv .liimg a{display: block;margin: 0.5em; cursor:default;}
.lileft .lidiv .liimg img {width: 100%; cursor:auto;}

.liright {float: right;width: 45.5%;margin-right: 3%;}
.liright .lidiv{float: left; background-color: #ffffff;border: 1px solid #d1d1d1;border-radius: 7px;box-shadow: 1px 1px 3px #d5d5d5;box-sizing: border-box;margin: 10px 0 0;position: relative;width: 100%;}
.liright .lidiv .liimg{text-align: center;}
.liright .lidiv .liimg a{display: block;margin: 0.5em; text-align: center;}
.liright .lidiv .liimg img {width: 100%;}

