.SQ_tc_txt li span{width:125px;height:140px;border:1px solid #ddd;padding:1px;float:left;margin:10px 0 0 134px;display:inline;}
.SQ_tc_txt li strong{width:400px;text-align:center;float:left;font-size:14px;font-weight:bold;font-family:"宋体";margin:10px 0 0 0;}
.SQ_tc_txt li p{width:400px;text-align:center;float:left;font-size:14px;font-family:"Arial";}
.SQ_tc_button{float:left;margin:10px 0 0 100px;_margin:10px 0 0 42px;padding:0 0 0 15px;background:url(http://base1.okimgs.com/images/wd_button.png) no-repeat 0 -253px;display:inline-block;height:37px;line-height:35px;}
.SQ_tc_button a{float:left;height:37px;line-height:35px;text-align:center;font-size:14px;color:#fff;font-family:"微软雅黑";background:url(http://base2.okimgs.com/images/wd_button.png) no-repeat right -305px;display:inline-block;padding:0 15px 0 0;}

/* 最新热卖 */
.mainHot{width:1128px;float:left;}
.mainHot li{width:1128px;float:left;margin:0 0 20px 0;}
.mainHot dl{float:left;width:134px;height:191px;padding:1px;border:1px solid #ddd;background:#fff;margin:0 20px 20px 0;display:inline;}
.mainHot dt{float:left;width:134px;height:134px;background:#fff;overflow:hidden;}
.mainHot dd{float:left;width:134px;height:57px;background:#fafafa;font-size:12px;font-family:"宋体";}
.mainHot dd span{float:left;width:128px;height:26px;line-height:26px;padding:0 0 0 6px;overflow:hidden;}
.mainHot dd p{float:left;width:128px;height:31px;line-height:31px;background:url(http://base3.okimgs.com/images/brand_three_bg.png) no-repeat;padding:0 0 0 6px;cursor:pointer;}
.mainHot dd p a font{float:left;font-size:12px;color:#fff;font-family:Verdana;}
.mainHot dd p a{float:left;font-size:14px;color:#fff;font-family:Arial;}
