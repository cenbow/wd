@charset "utf-8";
/* 公共样式 */
body,html,ul,li,dl,dt,dd,ol,h1,h2,h3,h4,h5,h6,p,form,input,th,td{margin:0px;padding:0px;}
/* 主体样式 */
body{font:12px/18px Arial,"宋体";color:#333;background:url(http://base.okimgs.com/images/W_bj.gif) repeat;}

/* 图片与表单*/
img{border:0px;}

/* 去点*/
ul,li{list-style-type:none;}
/* 链接样式*/
a{color:#666;text-decoration:none;}
a:hover{color:#f00;text-decoration:none;}
/* 公共调用*/
.clear{clear:both;height:0px;font:0px/0px;overflow:hidden;}
.left{float:left;}
.right{float:right;}
.fb{font-weight:bold;}
.orange{color:#F47A02;font-size:14px;font-family:Arial;}
.red{color:#e70002;font-size:12px;font-family:"宋体";}
 a:focus { outline: none; }/* a链接虚线 */
 
