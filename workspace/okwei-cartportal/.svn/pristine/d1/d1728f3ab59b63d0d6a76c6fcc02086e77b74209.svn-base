package com.okwei.cartportal.bean;

/**
 * 接收订单提交页面数据
 * @author yangjunjun
 *
 */
public class OrderSubmit {

	private long companyId;//供应商id
	private long scId;// 购物车ID
	private long productId;// 商品ID
	private int count;// 购买数量
    private long shopWeiId;// 来源微店
    private long stylesId;// 款式ID
    private String logisticsId;//选择的物流类型 
    private String message;//买家留言
    
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
	public long getScId() {
		return scId;
	}
	public void setScId(long scId) {
		this.scId = scId;
	}
	public long getProductId() {
		return productId;
	}
	public void setProductId(long productId) {
		this.productId = productId;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public long getShopWeiId() {
		return shopWeiId;
	}
	public void setShopWeiId(long shopWeiId) {
		this.shopWeiId = shopWeiId;
	}
	public long getStylesId() {
		return stylesId;
	}
	public void setStylesId(long stylesId) {
		this.stylesId = stylesId;
	}
	public long getCompanyId() {
		return companyId;
	}
	public void setCompanyId(long companyId) {
		this.companyId = companyId;
	}
	public String getLogisticsId() {
		return logisticsId;
	}
	public void setLogisticsId(String logisticsId) {
		this.logisticsId = logisticsId;
	}
    
}
