package com.okwei.bean.vo.activity;

import java.util.List;

import com.okwei.bean.domain.AActivityProducts;

public class AActivityProductsResult extends AActivityProducts implements java.io.Serializable{

	private static final long serialVersionUID = -7630372924359878101L;
	
	private String priceWithOutCommission;
	/**
	 * 抢购前的价格
	 */
	private Double priceBf;
	/**
	 * 原价
	 */
	private Double oldPrice;
	//卖家店铺名称
	private String shopName;
	private Integer sort;
	private Long actPid;
	public Long getActPid() {
		return actPid;
	}

	public void setActPid(Long actPid) {
		this.actPid = actPid;
	}

	public String getShopName() {
		return shopName;
	}

	public void setShopName(String shopName) {
		this.shopName = shopName;
	}

	/**
	 * 产品的抢购时间
	 */
	private List<AActivityProExtend> productExtends;
	//成交数量
	private int selledCount;
	//剩余库存
	private int stockCount;
	public int getStockCount() {
		return stockCount;
	}

	public void setStockCount(int stockCount) {
		this.stockCount = stockCount;
	}

	//成交金额
	private double selledAmount;
	
	public Double getPriceBf() {
		return priceBf;
	}

	public void setPriceBf(Double priceBf) {
		this.priceBf = priceBf;
	}


	public List<AActivityProExtend> getProductExtends() {
		return productExtends;
	}

	public void setProductExtends(List<AActivityProExtend> productExtends) {
		this.productExtends = productExtends;
	}

	public int getSelledCount() {
		return selledCount;
	}

	public void setSelledCount(int selledCount) {
		this.selledCount = selledCount;
	}

	public double getSelledAmount() {
		return selledAmount;
	}

	public void setSelledAmount(double selledAmount) {
		this.selledAmount = selledAmount;
	}

	public Double getOldPrice() {
		return oldPrice;
	}

	public void setOldPrice(Double oldPrice) {
		this.oldPrice = oldPrice;
	}

	public String getPriceWithOutCommission() {
		return priceWithOutCommission;
	}

	public void setPriceWithOutCommission(String priceWithOutCommission) {
		this.priceWithOutCommission = priceWithOutCommission;
	}

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

	
	
	
	
}
