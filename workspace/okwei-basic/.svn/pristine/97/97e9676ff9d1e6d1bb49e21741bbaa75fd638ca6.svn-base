package com.okwei.bean.enums;
/**
 * 购物车 订单类型
 * @author Administrator
 *
 */
public enum ShoppingCartTypeEnum {
	/**
	 * 零售订单
	 * */
	Retail(1),
	/**
	 * 预订单
	 * */
	Presell(2),
	/**
	 * 批发订单
	 * */
	Wholesale(3),
	/**
	 * 进货单
	 */
	Jinhuo(4),
	/**
	 * 铺货单
	 */
	Puhuo(5),
	/**
	 * 兑换
	 */
	Convert(6)
	;
	private final int step; 

    private ShoppingCartTypeEnum(int step) { 
         this.step = step; 
    }
    @Override
    public String toString()
    {
    	return String.valueOf(this.step);         	
    }
}
