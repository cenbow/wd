package com.okwei.bean.enums;

public enum LoginTypeEnum
{   
    /**
     * APP登陆
     */
    APPLogin(1),
    /**
     * web登陆
     */
    WEBLogin(2),
    /**
     * PC登陆
     */
    PCLogin(3);
    
    private final int step; 
    private LoginTypeEnum(int step) { 
         this.step = step; 
    }
    @Override
    public String toString()
    {
        return String.valueOf(this.step);               
    }
}
