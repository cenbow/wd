package com.okwei.bean.vo.order;

public class BReturnOdertInfo implements java.io.Serializable
{
    /**
	 * 下单返回结果信息
	 */
	private static final long serialVersionUID = 2707125117325866073L;
	/**
     * 
     */
     
    private String orderno;//订单号
    private Double alltotal;//订单总价
    private Double allpostage;//订单邮费
    private int state;//1成功， 非1 不成功
    private String msg; //原因
    
    

    public int getState() {
		return state;
	}
    

	public String getMsg() {
		return msg;
	}


	public void setMsg(String msg) {
		this.msg = msg;
	}


	public void setState(int state) {
		this.state = state;
	}

	public String getOrderno()
    {
        return orderno;
    }

    public void setOrderno(String orderno)
    {
        this.orderno = orderno;
    }

    public Double getAlltotal()
    {
        return alltotal;
    }

    public void setAlltotal(Double alltotal)
    {
        this.alltotal = alltotal;
    }

    public Double getAllpostage()
    {
        return allpostage;
    }

    public void setAllpostage(Double allpostage)
    {
        this.allpostage = allpostage;
    }
}
