package com.okwei.bean.vo.order;

import com.okwei.bean.enums.VerifyCodeType;


public class BSendSMSVO {
	/**
     * 发送的参数
     */
    private String[] param;
    /**
     * 微店号
     */
    private long weiid;
    /**
     * 类型枚举
     */
    private VerifyCodeType verify;

    public String[] getParam() {
	return param;
    }

    public void setParam(String[] param) {
	this.param = param;
    }

    public long getWeiid() {
	return weiid;
    }

    public void setWeiid(long weiid) {
	this.weiid = weiid;
    }

    public VerifyCodeType getVerify() {
	return verify;
    }

    public void setVerify(VerifyCodeType verify) {
	this.verify = verify;
    }
}
