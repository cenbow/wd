package com.okwei.bean.vo.product;

import java.util.List;

import com.okwei.bean.domain.PProductSellParamValues;

/**
 * 销售 属性 
 * @author Administrator
 *
 */
public class ProductSellParam {

	/**
	 * 属性id
	 */
	private int attributeId;
	/**
	 * 属性名称（如：颜色、尺码）
	 */
	private String name;
	/**
	 * 属性值 列表
	 */
	private List<PProductSellParamValues> vals;
	public int getAttributeId()
	{
		return attributeId;
	}
	public String getName()
	{
		return name;
	}
	public List<PProductSellParamValues> getVals()
	{
		return vals;
	}
	
	public void setAttributeId(int val)
	{
		this.attributeId=val;
	}
	public void setName(String val)
	{
		this.name=val;
	}
	public void setVals(List<PProductSellParamValues> val)
	{
		this.vals=val;
	}
}


