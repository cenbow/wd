package com.okwei.bean.enums;

public enum ShareTypeEnum {

	/**
	 * 简易型
	 */
	SimpleType(1),
	/**
	 * 单个精品
	 */
	SingleQuality (2),
	/**
	 * 多个精品
	 */
	MultipleQuality (3);

	private final int step;

	private ShareTypeEnum(int step) {

		this.step = step;
	}

	@Override
	public String toString() {
		return String.valueOf(this.step);
	}
}
