package com.okwei.bean.enums;

public enum BookPayTypeEnum {
	// / <summary>
	// / 全款
	// / </summary>
	Full(0),
	// / <summary>
	// / 百分比
	// / </summary>
	percent(1),
	// / <summary>
	// / 指定金额
	// / </summary>
	specifyamount(2);

	private final int step;

	private BookPayTypeEnum(int step) {

		this.step = step;
	}

	@Override
	public String toString() {
		return String.valueOf(this.step);
	}
}
