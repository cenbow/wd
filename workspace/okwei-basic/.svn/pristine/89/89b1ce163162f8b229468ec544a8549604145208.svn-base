package com.okwei.bean.enums;

/**
 * (铁杆粉丝申请)状态
 */
public enum FansStatusEnum
{
	/**
     * 申请中
     */
	Applying(0),
	/**
	 * 申请不通过
	 */
	Fail(2),
	/**
     * 申请通过
     */
    Pass(1);
  
    private final int Type;

    private FansStatusEnum(int step)
    {
        this.Type = step;
    }

    public String toString()
    {
        return String.valueOf(this.Type);
    }
}
