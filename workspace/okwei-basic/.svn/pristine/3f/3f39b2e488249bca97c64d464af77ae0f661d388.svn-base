package com.okwei.bean.enums;

/**
 * @author fuhao
 *	现金劵 状态枚举
 */
public enum WeiCoinLogStateEnum {
	
	//
	/**
	 * 待发
	 */
	releasing(0),
	/**
	 * /有效
	 */
	valid(1),
	/**
	 * 	//已到期 
	 */
	expired(2);
	
	private final int step;

	private WeiCoinLogStateEnum(int step) {
		this.step = step;
	}

	@Override
	public String toString() {
		return String.valueOf(this.step);
	}
}
