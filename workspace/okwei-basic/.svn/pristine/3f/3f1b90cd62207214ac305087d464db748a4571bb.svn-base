package com.okwei.bean.enums;

public enum RegionLevelEnum {
	
	/**
	 * 国家
	 */
	Country(0),
	/**
	 * 大区 华东 华北
	 */
	Region(1),
	/**
	 * 省
	 */
	Province(2),
	/**
	 * 市
	 */
	City(3),
	/**
	 * 区
	 */
	Area(4);
	private final int step; 

    private RegionLevelEnum(int step)
    { 
         this.step = step; 
    }
    @Override
    public String toString()
    {
    	return String.valueOf(this.step);         	
    }
}
