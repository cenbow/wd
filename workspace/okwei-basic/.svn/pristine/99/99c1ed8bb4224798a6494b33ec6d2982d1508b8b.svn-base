package com.okwei.bean.enums;

/**
 * @author fh 分享表S_MainData  onHomePage字段的枚举
 *
 */
public enum ShareOnHomePage {
	/**
	 * 上首页
	 */
	Yes (1),
	
	/**
	 * 未上首页
	 */
	No(2);
	
	private final int step;

	private ShareOnHomePage(int step) {

		this.step = step;
	}

	@Override
	public String toString() {
		return String.valueOf(this.step);
	}
}
